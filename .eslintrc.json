{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "import/no-anonymous-default-export": ["error", {
        "allowArray": false,
        "allowArrowFunction": false,
        "allowAnonymousClass": false,
        "allowAnonymousFunction": false,
        "allowCallExpression": true,
        "allowNew": false,
        "allowLiteral": false,
        "allowObject": false
    }]
},
}
